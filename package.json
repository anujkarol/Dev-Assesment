{
  "name": "teacher-admin",
  "version": "1.0.0",
  "description": "Api services for teachers to perform student administration  ",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register test/**/**/*.spec.ts",
    "start": "node dist/server.js",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon --watch src",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mysql": "github:mysqljs/mysql",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/mysql": "^2.15.5",
    "@types/sinon": "^7.0.10",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "concurrently": "^4.1.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "sinon": "^7.3.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "yarn": "^1.13.0"
  }
}
